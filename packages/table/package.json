{
  "name": "@vue-patternfly/table",
  "version": "0.2.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "concurrently 'npm:build:*'",
    "build:vite": "vite build",
    "build:bundle": "vite build -c vite-bundle.config.ts",
    "build:declarations": "vue-tsc --declaration --emitDeclarationOnly",
    "build:esm": "tsc --build --verbose tsconfig.json",
    "lint": "vue-tsc --noEmit",
    "watch": "concurrently 'npm:watch:*'",
    "watch:vite": "vite build --watch",
    "watch:tsc": "vue-tsc --declaration --emitDeclarationOnly --watch",
    "prepack": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@patternfly/react-styles": "^5.3.1",
    "@vue-patternfly/core": "workspace:^",
    "@vue-patternfly/icons": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/compiler-sfc": "^3.4.31",
    "@vueuse/core": "^10.11.0",
    "glob": "^10.4.2",
    "rimraf": "^5.0.7",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vue": "^3.4.31",
    "vue-tsc": "^2.0.24"
  },
  "peerDependencies": {
    "@vue-patternfly/core": "workspace:^",
    "@vueuse/core": "^10"
  }
}
